<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?><?import javafx.scene.layout.VBox?>
<VBox maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.mikesajak.ebooklib.app.ui.BookTableController">
   <children>
      <ToolBar prefHeight="40.0" prefWidth="200.0" VBox.vgrow="NEVER">
        <items>
          <Button fx:id="editMetaButton" mnemonicParsing="false" onAction="#onImportBookAction" text="Import book" />
        </items>
      </ToolBar>
      <TableView fx:id="booksTableView" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" minHeight="100.0" minWidth="-Infinity" VBox.vgrow="ALWAYS">
        <columns>
          <TableColumn fx:id="titleColumn" prefWidth="101.0" text="%book_table_panel.title_column.label" />
          <TableColumn fx:id="authorsColumn" prefWidth="145.0" text="%book_table_panel.authors_column.label" />
            <TableColumn fx:id="tagsColumn" minWidth="8.0" prefWidth="67.0" text="%book_table_panel.tags_column.label" />
            <TableColumn fx:id="identifiersColumn" prefWidth="133.0" text="%book_table_panel.identifiers_column.label" />
            <TableColumn fx:id="creationDateColumn" minWidth="0.0" prefWidth="119.0" text="%book_table_panel.created_column.label" />
            <TableColumn fx:id="publicationDateColumn" prefWidth="118.0" text="%book_table_panel.publication_column.label" />
            <TableColumn fx:id="publisherColumn" prefWidth="121.0" text="%book_table_panel.publisher_column.label" />
            <TableColumn fx:id="languagesColumn" prefWidth="126.0" text="%book_table_panel.languages_column.label" />
        </columns>
      </TableView>
   </children>
</VBox>
